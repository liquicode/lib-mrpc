{
  "name": "@liquicode/lib-mrpc",
  "version": "0.0.1",
  "description": "A transport agnostic message based remote procedure call library designed to facilitate the evolution and scalability of complex systems.",
  "license": "MIT",
  "homepage": "http://lib-mrpc.liquicode.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/liquicode/lib-mrpc.git"
  },
  "authors": [
    {
      "name": "Andre' G. Bowlin",
      "email": "agbowlin@gmail.com",
      "homepage": "http://liquicode.com"
    }
  ],
  "main": "lib-mrpc.js",
  "files": [
    "readme.md",
    "VERSION",
    "LICENSE",
    "lib-mrpc.js",
    "src/*",
    "docs/*",
    "tests/*"
  ],
  "ignore": [
    ".*",
    "~*",
    "*.dockerfile",
    "bower_components",
    "node_modules",
    "build"
  ],
  "keywords": [
    "javascript",
    "js",
    "nodejs",
    "node",
    "rpc",
    "remote procedure call",
    "message",
    "message broker",
    "thread",
    "amqp",
    "stomp"
  ],
  "dependencies": {},
  "devDependencies": {
    "@vivaxy/here": "^3.4.0",
    "amqplib": "^0.6.0",
    "docdash": "^1.2.0",
    "jsdoc": "^3.6.6",
    "mocha": "^7.1.2",
    "redis": "^3.0.2",
    "stompit": "^1.0.0",
    "tortoise": "^1.0.1",
    "uniqid": "^5.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "run-tests": "mocha -u bdd tests --timeout 0 --slow 0 --colors",
    "run-docs-web-server": "npx here --directory docs --port 4200 --watch 1 --silent",
    "010-webpack": "bash build/webpack/010-webpack.sh",
    "020-gen-lib-docs": "node build/docs/020-gen-lib-docs.js ./docs ./src ./tests",
    "021-gen-jsdoc-web": "npx jsdoc -c build/__secrets/jsdoc.config.json",
    "022-gen-jsdoc-json": "npx jsdoc -c build/__secrets/jsdoc.config.json -X > docs/docs.json",
    "810-s3-sync-docs": "bash build/s3/810-s3-sync-docs.sh",
    "900-npm-publish-public": "bash build/npm/900-npm-publish-public.sh"
  }
}
